{
  "name": "Complaint",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "comment": {
      "type": "string"
    },
    "id": {
      "type": "number",
      "id": true
    },
    "when": {
      "type": "date",
      "required": true
    },
    "addAt": {
      "type": "date",
      "required": false,
      "defaultFn":"now"
    },
    "location": {
      "type": "geopoint",
      "required": true
    },
    "userName": {
      "type": "string",
      "required": true
    },
    "imgUrl": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
      "user": {
      "type": "belongsTo",
      "model": "User",
      "foreignKey": ""
    }
      
      
  },
  "acls": [],
  "methods": []
}
